<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Delta.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Delta"
    xmlns:views="clr-namespace:Delta.Views">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <ShellContent
        Title="TestPage"
        ContentTemplate="{DataTemplate views:TestPage}"
        Route="TestPage" />

    <Shell.FlyoutHeader>
        <Grid>
            <Label Text="Vorname2"
                   FontSize="16"/>
        </Grid>
    </Shell.FlyoutHeader>
</Shell>
